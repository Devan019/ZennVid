  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
  },
  "include": [
    "src",
    "remotion",
    "src/index.tsx",
    "src/remotionVideo.tsx",
    "src/service.ts",
    "src/server.ts"
  ]
}